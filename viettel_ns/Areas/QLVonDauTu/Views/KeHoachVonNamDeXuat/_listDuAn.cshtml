@using Viettel.Models.QLVonDauTu
@model List<VDTDuAnViewModel>

@{
    foreach (var item in Model)
    {
        <tr>
            <td align="center">
                <input type="checkbox" data-idduan="@item.iID_DuAnID" class="cb_duan"  />
            </td>
            <td>@item.sMaDuAn</td>
            <td>@item.sTenDuAn</td>
            <td>@item.sTenLoaiDuAn</td>
        </tr>
    }
}
